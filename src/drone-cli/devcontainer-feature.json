{
    "id": "drone-cli",
    "version": "1.0.7",
    "name": "Drone CLI (via Github Releases)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/drone-cli",
    "description": "Drone CLI is a command line client for the Drone continuous integration server.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/gh-release"
    ]
}